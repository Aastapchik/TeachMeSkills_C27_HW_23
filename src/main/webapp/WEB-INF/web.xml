<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
    <servlet-name>MyFirstServlet</servlet-name>
    <servlet-class>com.timeTask.TimeMinsk</servlet-class>
  </servlet>

  <servlet>
    <servlet-name>MySecondServlet</servlet-name>
    <servlet-class>com.timeTask.TimeWashington</servlet-class>
  </servlet>


  <servlet-mapping>
    <servlet-name>MyFirstServlet</servlet-name>
    <url-pattern>/minsk</url-pattern>
  </servlet-mapping>

  <servlet-mapping>
    <servlet-name>MySecondServlet</servlet-name>
    <url-pattern>/washington</url-pattern>
  </servlet-mapping>



</web-app>
